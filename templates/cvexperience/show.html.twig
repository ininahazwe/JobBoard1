{% extends 'base-dashboard.html.twig' %}

{% block title %}CvExperience{% endblock %}

{% block body %}
    <h1>CvExperience</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Post</th>
                <td>{{ cv_experience.post }}</td>
            </tr>
            <tr>
                <th>Entreprise</th>
                <td>{{ cv_experience.entreprise }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ cv_experience.debut ? cv_experience.debut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ cv_experience.fin ? cv_experience.fin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ cv_experience.description|raw }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cvexperience_index') }}">back to list</a>

    <a href="{{ path('cvexperience_edit', {'id': cv_experience.id}) }}">edit</a>

    {{ include('cvexperience/_delete_form.html.twig') }}
{% endblock %}
